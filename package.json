{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "website",
  "title": "Website",
  "description": "Open a website page in different environments: production, development, storyblok",
  "icon": "extension-icon.png",
  "author": "XylophoneHero",
  "owner": "webinargeek",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "open-website",
      "title": "Open Website",
      "description": "Open a website link in a different environment",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "previewToken",
      "title": "Preview Token",
      "description": "Storyblok CDN v2 preview access token. Paste in the raycast token from (https://app.storyblok.com/#/me/spaces/110864/settings?tab=api)",
      "type": "password",
      "required": true
    },
    {
      "name": "spaceId",
      "title": "Space ID",
      "description": "Storyblok Space ID",
      "type": "textfield",
      "required": true,
      "default": "110864"
    },
    {
      "name": "productionUrl",
      "title": "Production URL",
      "description": "Production website base URL",
      "type": "textfield",
      "required": true,
      "default": "https://www.webinargeek.com"
    },
    {
      "name": "newProductionUrl",
      "title": "New production URL",
      "description": "New production website base URL",
      "type": "textfield",
      "required": true,
      "default": "https://schumi.vercel.app"
    },
    {
      "name": "developmentUrl",
      "title": "Development URL",
      "description": "Local development base URL",
      "type": "textfield",
      "required": true,
      "default": "https://localhost:4321"
    },
    {
      "name": "gatsbyDevelopmentUrl",
      "title": "Gatsby Development URL",
      "description": "Gatsby development base URL",
      "type": "textfield",
      "required": true,
      "default": "https://localhost:8000"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.7",
    "@raycast/utils": "^2.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "~25.3.2",
    "@types/react": "~19.2.14",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
